{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Spring Boot-WithMeBackendApplication",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "com.javalab.withme.WithMeBackendApplication", // 메인 클래스 경로
      "projectName": "withme_backend", // 프로젝트 이름
      "args": "",
      "envFile": "${workspaceFolder}/.env",
      "vmArgs": "-Dserver.port=8080" // 서버 포트를 명시적으로 설정 (필요 시 수정)
    }
  ]
}
